{
  "compilerOptions": {
    "outDir": "dist",
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "incremental": true,
    "target": "es6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
